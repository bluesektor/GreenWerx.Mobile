{
  "name": "GreenWerx",
  "version": "0.0.9",
  "author": "Ionic Framework",
  "homepage": "https://greenwerx.org/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ionic:build": "ng build --output-hashing=all --prod --source-map --named-chunks && gzipper ./www ./gzipped",
    "ionic:serve": "ng serve --prod --verbose",
    "test-pwa": "npm run build && http-server dist -p 8080 --gzip --push-state",
    "bundle-report": "ng build --prod --verbose --stats-json && webpack-bundle-analyzer www/stats.json",
    "unused-report": "ng build --prod --verbose --stats-json && webpack --json | webpack-unused -s www",
    "source-map": "ng build --prod --sourceMap",
    "webpack-jarvis": "ng build --prod --verbose --stats-json && webpack --json --config ./webpack/webpack.jarvis.js -s www"
  },
